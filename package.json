{
  "name": "nba21-frontend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "cracoConfig": "craco.config.js",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.1",
    "axios": "^0.21.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^26.6.0",
    "jest-dom": "^4.0.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "mobx": "^6.12.3",
    "node-sass": "^6.0.0",
    "pint": "^0.2.1",
    "prettier": "^3.2.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-countdown-circle-timer": "^2.5.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-test-renderer": "^17.0.2",
    "storybook-preset-craco": "^0.0.6",
    "styled-components": "^5.3.3",
    "typescript": "^4.9.5",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/node-logger": "^6.1.16",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.16",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "husky": "^4.3.8",
    "lodash": "^4.17.21",
    "mobx-react": "^9.1.1",
    "puppeteer": "^13.0.1",
    "react-mobx": "^0.0.4"
  },
  "scripts": {
    "start": "craco start",
    "start:dev": "SET PORT=3001 && SET REACT_APP_MODE=development && craco start",
    "start:dev:mac": "PORT=3001 REACT_APP_MODE=development craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "storybook": "SET STORYBOOK_APP_MODE=development && start-storybook -p 6006 -s public",
    "storybook:mac": "STORYBOOK_APP_MODE=development start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "craco test --testPathIgnorePatterns=.*e2e.test.js --watchAll=false",
    "test:watch": "craco test --testPathIgnorePatterns=.*e2e.test.js",
    "test:e2e": "SET REACT_APP_MODE=development && craco test --testPathPattern=.*e2e.test.js$ --watchAll=false",
    "test:e2e:mac": "REACT_APP_MODE=development craco test --testPathPattern=.*e2e.test.js$ --watchAll=false",
    "test:e2e:watch": "SET REACT_APP_MODE=development && craco test --testPathPattern=.*e2e.test.js$",
    "test:all": "craco test --watchAll=false"
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.16.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^@helpers(.*)$": "<rootDir>/src/helpers$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@activities(.*)$": "<rootDir>/src/activities$1",
      "^@shared_activities(.*)$": "<rootDir>/src/activities/shared$1",
      "^@config(.*)$": "<rootDir>/src/config$1",
      "^@pages(.*)$": "<rootDir>/src/pages$1"
    }
  }
}
